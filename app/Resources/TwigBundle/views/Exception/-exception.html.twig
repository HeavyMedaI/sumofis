<script type="text/javascript" src="{{ asset('bundles/Makrosum/Meeting/_error/ouppss/') }}js/modernizr.js"></script>
<script type="text/javascript" src="{{ asset('bundles/Makrosum/Meeting/_error/ouppss/') }}js/jquery.validate.js"></script>

<!-- THEME PLUGIN -->

<script type="text/javascript" src="{{ asset('bundles/Makrosum/Meeting/_error/ouppss/') }}js/ouppss_ui.js"></script>

<!-- PLUGINS -->

</body>
</html>
